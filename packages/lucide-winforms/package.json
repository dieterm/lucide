{
  "name": "lucide-winforms",
  "description": "Lucide icons as WinForms .resx Resource files for .NET applications.",
  "version": "0.0.1",
  "license": "ISC",
  "homepage": "https://lucide.dev",
  "bugs": "https://github.com/lucide-icons/lucide/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucide-icons/lucide.git",
    "directory": "packages/lucide-winforms"
  },
  "keywords": [
    "Lucide",
    "Icons",
    "WinForms",
    "Windows Forms",
    ".NET",
    "Resources",
    "RESX"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm copy:license && pnpm build:resources",
    "copy:license": "node -e \"require('fs').copyFileSync('../../LICENSE', './LICENSE')\"",
    "clean": "node -e \"const fs=require('fs');['dist','obj','bin'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch(e){}})\"",
    "build:resources": "dotnet run --project ./scripts/GenerateResources.csproj"
  },
  "devDependencies": {},
  "engines": {
    "node": ">= 20"
  }
}
